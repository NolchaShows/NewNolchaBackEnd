{
  "kind": "collectionType",
  "collectionName": "experience_pages",
  "info": {
    "singularName": "experience-page",
    "pluralName": "experience-pages",
    "displayName": "Experience Page",
    "description": "Dynamic experience pages rendered by the Next.js route /experiences/[slug]."
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true,
      "unique": true
    },
    "seo": {
      "type": "component",
      "component": "shared.seo",
      "required": false
    },
    "hero": {
      "type": "component",
      "component": "experience.hero",
      "required": false
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "blocks.rich-text",
        "blocks.image-text",
        "blocks.three-image-row",
        "blocks.gallery"
      ],
      "required": false
    }
  }
}
